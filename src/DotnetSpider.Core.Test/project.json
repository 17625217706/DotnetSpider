{
  "version": "1.0.0",
  "testRunner": "xunit",
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable",
    "copyToOutput": [
      "config.ini",
      "nlog.netcore.config",
      "nlog.net45.config",
      "Jd.html",
      "xunit.runner.json"
    ]
  },

  "publishOptions": {
    "includeFiles": [ "config.ini", "nlog.netcore.config", "nlog.net45.config", "Jd.html" ]
  },
  "dependencies": {
    "DotnetSpider.Core": "0.0.9",
    "Dapper": "1.50.2",
    "xunit": "2.2.0-beta3-build3402",
    "xunit.runner.visualstudio": "2.2.0-beta3-build1187",
    "xunit.runner.console": "2.2.0-beta3-build3402"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8+dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.Net.Http": "4.1.0",
        "System.Diagnostics.Process": "4.1.0",
        "Microsoft.Extensions.Logging.Abstractions": "1.0.0",
        "System.Text.Encoding.CodePages": "4.0.1",
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "SapientGuardian.MySql.Data": "6.9.812",
        "System.Linq": "4.1.0"
      },
      "buildOptions": {
        "define": [
          "RELEASE",
          "NET_CORE"
        ]
      }
    },
    "net45": {
      "dependencies": {
        "MySql.Data": "6.9.9"
      },
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      },
      "buildOptions": {
        "define": [
          "RELEASE",
          "NET_45"
        ]
      }
    }
  }
}